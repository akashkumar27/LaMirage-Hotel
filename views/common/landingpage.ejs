    <section id="title">
      <div class="container-fluid">
        <!-- Title -->
        <div class="row">
          <div class="col-lg-6">
            <h1 class="big-heading">Explore your place to stay</h1>
            <div class="row">
              <div class="col-6">
                <a
                  class="btn btn-secondary btn-lg download-buttons"
                  href="/signin"
                  role="button"
                  >Sign in</a
                >
              </div>

              <div class="col-6">
                <a
                  class="btn btn-dark btn-lg download-buttons"
                  href="/signup"
                  role="button"
                  >Sign up</a
                >
              </div>
            </div>
          </div>
          <div class="col-lg-6">
            <img class="hotel-img" src="images/hotel.jpg" alt="hotel-img" />
          </div>
        </div>
      </div>
    </section>

    <section id="features">
      <div class="row">
        <div class="feature-box col-lg-4">
          <i class="ft-icon fas fa-check-circle fa-4x"></i>
          <h3>Fast booking.</h3>
          <p>Book and confirm rooms in seconds.</p>
        </div>
        <div class="feature-box col-lg-4">
          <i class="ft-icon fas fa-bullseye fa-4x"></i>
          <h3>High class ambience</h3>
          <p>Enjoy top class luxury facilities.</p>
        </div>
        <div class="feature-box col-lg-4">
          <i class="ft-icon fas fa-heart fa-4x"></i>
          <h3>Affordable rate</h3>
          <p>Get best rooms at affordable rate.</p>
        </div>
      </div>
    </section>

    <section id="testimonials">
      <div id="testimonial-carousel" class="carousel slide">
        <div class="carousel-inner">
          <div class="carousel-item active">
            <h2 class="testimonial-text">
              Wonderful experience! Great service
            </h2>
            <img
              class="testimonial-img"
              src="images/person-logo.png"
              alt="people-profile"
            />
            <em>Ram, Chennai</em>
          </div>
          <div class="carousel-item">
            <h2 class="testimonial-text">
              Defenitely worth for money, Go for it
            </h2>
            <img
              class="testimonial-img"
              src="images/person-logo.png"
              alt="people-profile"
            />
            <em>Rajesh, Madurai</em>
          </div>
        </div>
        <button
          class="carousel-control-prev"
          type="button"
          data-target="#testimonial-carousel"
          data-slide="prev"
        >
          <span
            class="carousel-control-prev-icon ctrl-btn"
            aria-hidden="true"
          ></span>
          <span class="sr-only">Previous</span>
        </button>
        <button
          class="carousel-control-next"
          type="button"
          data-target="#testimonial-carousel"
          data-slide="next"
        >
          <span
            class="carousel-control-next-icon ctrl-btn"
            aria-hidden="true"
          ></span>
          <span class="sr-only">Next</span>
        </button>
      </div>
    </section>

